{
    "name": "OCI Chatbot",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
      "storage",
      "https://150.230.44.54/*",
      "clipboardWrite",
      "microphone"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "web_accessible_resources": [
      {
        "resources": ["index.html", "script.js", "styles.css", "botAvatar.png", "userAvatar.png", "message-received.mp3"],
        "matches": ["<all_urls>"]
      }
    ]
  }
  